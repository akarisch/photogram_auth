<h1> index users here </h1>

<table>
<% @users.each do |user| %>
<tr>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
  <td>
    <a href="/users/<%= user.id %>" class="btn btn-primary">Show</a>
  <% if current_user.id == user.id %>
    <a href="/users/<%= user.id %>/edit" class="btn btn-warning">Edit</a>
  <% end %>
  </td>
</tr>
<% end %>
</table>
